{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "defaultDestination": "wwwroot/lib",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jquery-validation"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive"
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-easing@1.4.1",
      "destination": "wwwroot/lib/jquery-easing"
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.8.2",
      "destination": "wwwroot/lib/fontawesome"
    },
    {
      "provider": "cdnjs",
      "library": "tooltipster@3.3.0",
      "destination": "wwwroot/lib/tooltipster"
    },
    {
      "provider": "cdnjs",
      "library": "datatables@1.10.20",
      "destination": "wwwroot/lib/datatables"
    },
    {
      "provider": "cdnjs",
      "library": "moment.js@2.24.0",
      "destination": "wwwroot/lib/momentjs"
    },
    {
      "provider": "cdnjs",
      "library": "bootbox.js@4.4.0",
      "destination": "wwwroot/lib/bootboxjs"
    },
    {
      "provider": "unpkg",
      "library": "chart.js@2.8.0",
      "destination": "wwwroot/lib/chartjs",
      "files": [
        "dist/Chart.css",
        "dist/Chart.js",
        "dist/Chart.min.css",
        "dist/Chart.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker"
    }
  ]
}